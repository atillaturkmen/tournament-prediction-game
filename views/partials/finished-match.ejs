<td> 
    <div class="tahmin">
        <div class="tahmin-iy">
            <div class="tahmin-home">
                <span class="team-name"><%= home_team %> &nbsp</span> <img src=<%= home_team_logo %> alt=""> &nbsp
            </div>
            <div class="tahmin-score">
                <% if (show) { %>
                    <%= iy_home %>-<%= iy_away %>
                <% } else { %>
                    ? - ?
                <% } %>
            </div>
            <div class="tahmin-away">
                &nbsp <img src=<%= away_team_logo %> alt=""> <span class="team-name"> &nbsp <%= away_team %></span>
            </div>
        </div>
        <div class="tahmin-ms">
            <div class="tahmin-home">
                <span class="team-name"><%= home_team %> &nbsp</span> <img src=<%= home_team_logo %> alt=""> &nbsp
            </div>
            <div class="tahmin-score">
                <% if (show) { %>
                    <%= ms_home %>-<%= ms_away %>
                <% } else { %>
                    ? - ?
                <% } %>
            </div>
            <div class="tahmin-away">
                &nbsp <img src=<%= away_team_logo %> alt=""> <span class="team-name"> &nbsp <%= away_team %></span>
            </div>
        </div>
    </div>
</td>